<SCENE>

<!-- Initial values -->
<INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="5" />
</INITIALS>


<!-- Illumination and clear colour -->
<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
</ILLUMINATION>


<!-- Lights -->
<LIGHTS>
	<LIGHT id="light0">
        <enable value ="1" />
        <position  x="0" y="10" z="0" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

	<LIGHT id="light1">
        <enable value ="1" />
        <position x="30" y="10" z="0" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

	<LIGHT id="light2">
        <enable value ="1" />
        <position x="0" y="10" z="20" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light3">
        <enable value ="1" />
        <position x="30" y="10" z="20" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
        <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

</LIGHTS>

<!-- Textures -->
<TEXTURES>
	<!-- Tree trunk -->
    <TEXTURE id="trunk">
        <file path="images/trunk-image.jpg" />
        <amplif_factor s="2" t="2" />
    </TEXTURE>

     <!-- Tree leaves -->
    <TEXTURE id="folhas">
        <file path="images/leaves.jpg" />
        <amplif_factor s="3" t="4" />
    </TEXTURE>

    <!-- Rocks -->
    <TEXTURE id="rocks">
        <file path="images/rocks.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Glass work -->
    <TEXTURE id="vidral">
        <file path="images/vidral.jpg" />
        <amplif_factor s="15" t="10" />
    </TEXTURE>

    <!-- Street bench -->
    <TEXTURE id="banco">
        <file path="images/bank.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

</TEXTURES>


<MATERIALS>
	<!-- Pyramid material -->
    <MATERIAL id="piramideMaterial">
        <shininess value="1" />
        <specular r="0" g="0" b="0" a="1" />
        <diffuse r="1.0" g="0.68" b="0.4" a="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

	<!-- Tree leaves material -->
	<MATERIAL id="leavesTreeMaterial">
        <shininess value="1" />
        <specular r="0.2" g="0.2" b="0.2" a="1" />
        <diffuse r="0.0" g="0.207" b="0.0" a="1" />
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

	<!-- Tree trunk material -->
	<MATERIAL id="trunkTreeMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.6" g="0.298" b="0.0" a="1" />
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Lamp post material -->
    <MATERIAL id="lightPostMaterial">
        <shininess value="100" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.447" g="0.416" b="0.416" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Light bulb material -->
    <MATERIAL id="lightMaterial">
        <shininess value="100" />
        <specular r="0.5" g="0.5" b="0.5" a="1" />
        <diffuse r="0.5" g="0.5" b="0.5" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />
        <ambient r="0.4" g="0.4" b="0.4" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

</MATERIALS>


<!-- The leaves -->
<LEAVES>
</LEAVES>

<ANIMATIONS>

    <ANIMATION id="updown" speed="2" type="linear">
        <controlpoint xx="0" yy="0" zz="0"/>
        <controlpoint xx="3" yy="0" zz="2"/>
        <controlpoint xx="3" yy="-1" zz="2"/>
        <controlpoint xx="3" yy="0" zz="2"/>
        <controlpoint xx="3" yy="3" zz="2"/>
        <controlpoint xx="3" yy="0" zz="2"/>
    </ANIMATION>

    <ANIMATION id="leftright" speed="2" type="linear">
        <controlpoint xx="0" yy="0" zz="0"/>
        <controlpoint xx="2" yy="0" zz="2"/>
        <controlpoint xx="0" yy="0" zz="0"/>
        <controlpoint xx="2" yy="0" zz="0"/>
        <controlpoint xx="0" yy="0" zz="2"/>
    </ANIMATION>

    <ANIMATION id="bez" speed="3" type="bezier">
      <controlpoint xx="0" yy="0" zz="0"/>
      <controlpoint xx="0" yy="10" zz="10"/>
      <controlpoint xx="-5" yy="0" zz="-10"/>
      <controlpoint xx="5" yy="0" zz="0"/>
    </ANIMATION>

    <ANIMATION id="rodar" speed="4" type="circular" centerx="0" centery="0"
      centerz="0" radius="5" startang="0" rotang="90"/>

    <ANIMATION id="combination" type="combo">
      <SPANREF id="leftright"/>
      <SPANREF id="rodar"/>
      <SPANREF id="updown"/>
      <SPANREF id="bez"/>
    </ANIMATION>

</ANIMATIONS>

<!-- The nodes -->
<NODES>

    <ROOT id="root" />

    <!-- The root node -->
    <NODE id="root">
        <MATERIAL id="defaultMaterial" />
        <TEXTURE id="null" />

        <DESCENDANTS>
            <NODEREF id="bancos" />
        </DESCENDANTS>

    </NODE>

    <!-- Street bench -->
    <NODE id="bancos">
        <MATERIAL id="lightMaterial" />
        <TEXTURE id="banco" />


        <DESCENDANTS>
          <NODEREF id="bola" />
          <NODEREF id="banco1" />
          <NODEREF id="seta" />
          <NODEREF id="poste" />


        </DESCENDANTS>

    </NODE>

    <!-- Another bench -->
     <NODE id="banco1" selectable="true">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <ANIMATION id="leftright"/>
        <DESCENDANTS>
            <NODEREF id="banco" />
        </DESCENDANTS>
    </NODE>

    <!-- Another bench -->
     <NODE id="bola" selectable="true">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <ANIMATION id="rodar"/>
        <ROTATION axis="x" angle="90"/>
        <DESCENDANTS>
          <NODEREF id="banco" />
        </DESCENDANTS>
    </NODE>

    <NODE id="poste" selectable="true">
       <MATERIAL id="null" />
       <TEXTURE id="null" />
       <ANIMATION id="bez"/>
       <ROTATION axis="x" angle="90"/>
       <DESCENDANTS>
          <LEAF type="cylinder" args="10 2 2 20 20 1 1"/>
       </DESCENDANTS>
   </NODE>

   <NODE id="seta" selectable="true">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <ANIMATION id="combination"/>
      <DESCENDANTS>
        <LEAF id="triangle1" type="triangle" args="2 0 12 12 0 2 0 0 0" />
      </DESCENDANTS>
  </NODE>

    <!-- Generic bench -->
    <NODE id="banco">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <ROTATION axis="y" angle="90" />
        <SCALE sx="0.5" sy="0.5" sz="1.5" />

        <DESCENDANTS>

       		<NODEREF id="faceCubo1" />
            <NODEREF id="faceCubo2" />
            <NODEREF id="faceCubo3" />
            <NODEREF id="faceCubo4" />
            <NODEREF id="faceCubo5" />
            <NODEREF id="faceCubo6" />
        </DESCENDANTS>

    </NODE>


    <!-- A cube face -->
    <NODE id="faceCubo1">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="2.5" y="4" z="7.5" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="0 4 5 0" />
        </DESCENDANTS>

    </NODE>

    <!-- Another cube face -->
     <NODE id="faceCubo2">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="2.5" y="4" z="2.5" />
		<ROTATION axis="y" angle="-90" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="0 4 5 0" />
        </DESCENDANTS>

    </NODE>

    <!-- Another cube face -->
    <NODE id="faceCubo3">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="7.5" y="4" z="7.5" />
		<ROTATION axis="y" angle="90" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="0 4 5 0" />
        </DESCENDANTS>

    </NODE>

    <!-- Another cube face -->
    <NODE id="faceCubo4">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="7.5" y="4" z="2.5" />
        <ROTATION axis="y" angle="180" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="0 4 5 0" />
        </DESCENDANTS>

    </NODE>

    <!-- Another cube face -->
    <NODE id="faceCubo5">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <ROTATION axis="x" angle="-90" />
        <TRANSLATION x="2.5" y="-7.5" z="8" />
        <SCALE sx="1" sy="1.25" sz="1" />

        <DESCENDANTS>
 					<LEAF  type="rectangle" args="0 4 5 0" />
       </DESCENDANTS>

    </NODE>

    <!-- Another cube face -->
    <NODE id="faceCubo6">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="2.5" y="4" z="2.5" />
        <ROTATION axis="x" angle="90" />
        <SCALE sx="1" sy="1.25" sz="1" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="0 4 5 0" />
        </DESCENDANTS>

    </NODE>



</NODES>

</SCENE>
