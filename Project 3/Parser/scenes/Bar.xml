        <LIGHT id="light2">
            <enable value ="1" />
            <position x="0" y="10" z="20" w="1" />
            <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
            <diffuse r="0" g="0" b="1.0" a="1.0" />
            <specular r="0" g="0" b="1.0" a="1.0" />
