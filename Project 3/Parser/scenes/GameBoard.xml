<SCENE>

  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="5" />
  </INITIALS>

  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
  </ILLUMINATION>

  <LIGHTS>
    <LIGHT id="Top-Left light">
      <enable value ="1" />
      <position x="0" y="10" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="Bottom-Left light">
      <enable value ="1" />
      <position x="0" y="10" z="18" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="Bottom-Right light">
      <enable value ="1" />
      <position x="48" y="10" z="18" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="Top-Right light">
      <enable value ="1" />
      <position x="48" y="10" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="Middle light">
      <enable value ="1" />
      <position x="24" y="10" z="9" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>
  </LIGHTS>

  <TEXTURES>

    <TEXTURE id="vidral">
      <file path="images/vidral.jpg" />
      <amplif_factor s="15" t="10" />
    </TEXTURE>

  </TEXTURES>

  <MATERIALS>

    <MATERIAL id="piramideMaterial">
      <shininess value="1" />
      <specular r="0" g="0" b="0" a="1" />
      <diffuse r="1.0" g="0.68" b="0.4" a="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

  </MATERIALS>

  <LEAVES>

  </LEAVES>

  <ANIMATIONS>

  </ANIMATIONS>

  <NODES>

    <ROOT id="root" />

    <NODE id="root">
      <MATERIAL id="piramideMaterial" />
      <TEXTURE id="null" />
      <DESCENDANTS>
        <NODEREF id="Tabuleiro" />
      </DESCENDANTS>
    </NODE>

    <NODE id="Tabuleiro">
      <MATERIAL id="piramideMaterial" />
      <TEXTURE id="null" />
      <DESCENDANTS>
        <NODEREF id="BoardCube" />
      </DESCENDANTS>
    </NODE>

    <NODE id="BoardCube">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <TRANSLATION x="0" y="0" z="-3" />
      <DESCENDANTS>
       	<NODEREF id="faceCubo1" />
        <NODEREF id="faceCubo2" />
        <NODEREF id="faceCubo3" />
        <NODEREF id="faceCubo4" />
        <NODEREF id="faceCubo5" />
        <NODEREF id="faceCubo6" />
      </DESCENDANTS>
    </NODE>

    <NODE id="faceCubo1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <ROTATION axis="y" angle="180" />
      <DESCENDANTS>
				<LEAF  type="rectangle" args="-3 3 3 -3" />
      </DESCENDANTS>
    </NODE>

    <NODE id="faceCubo2">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <TRANSLATION x="3" y="0" z="3" />
      <ROTATION axis="y" angle="90" />
      <DESCENDANTS>
				<LEAF  type="rectangle" args="-3 3 3 -3" />
      </DESCENDANTS>
    </NODE>

    <NODE id="faceCubo3">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <TRANSLATION x="0" y="0" z="6" />
      <DESCENDANTS>
				<LEAF  type="rectangle" args="-3 3 3 -3" />
      </DESCENDANTS>
    </NODE>

    <NODE id="faceCubo4">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <TRANSLATION x="-3" y="0" z="3" />
      <ROTATION axis="y" angle="-90" />
      <DESCENDANTS>
				<LEAF  type="rectangle" args="-3 3 3 -3" />
      </DESCENDANTS>
    </NODE>

    <NODE id="faceCubo5">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <TRANSLATION x="0" y="3" z="3" />
      <ROTATION axis="x" angle="-90" />
      <DESCENDANTS>
				<LEAF  type="rectangle" args="-3 3 3 -3" />
      </DESCENDANTS>
    </NODE>

    <NODE id="faceCubo6">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <TRANSLATION x="0" y="-3" z="3" />
      <ROTATION axis="x" angle="90" />
      <DESCENDANTS>
				<LEAF  type="rectangle" args="-3 3 3 -3" />
      </DESCENDANTS>
    </NODE>

  </NODES>

</SCENE>
